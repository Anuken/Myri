#ifdef GL_ES
#define LOWP lowp
    precision mediump float;
#else
    #define LOWP
#endif

varying vec2 v_texCoords;

uniform sampler2D u_texture;

float round(float i, float s){
	return int(i/s)*s;
}

void main(){
	float s = 1.0/1440*10;
	vec2 uv = v_texCoords;
	uv.x = round(uv.x, 1.0/1440*10);
	uv.y = round(uv.y, 1.0/900*10);

    gl_FragColor = texture2D(u_texture, uv);
}
